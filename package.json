    "build-bundle": "vue-cli-service build --target lib -name laravel-vue-crud ./src/build.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bulma": "^0.7.5",
    "core-js": "^2.6.11",
    "material-design-icons": "^3.0.1",
    "noty": "^3.2.0-beta-deprecated",
    "quill": "^1.3.7",
    "quill-image-resize": "^3.0.9",
    "quill-image-upload": "^0.1.3",
    "vue": "^2.6.11",
    "vue-flatpickr-component": "^8.1.5",
    "vue-js-toggle-button": "^1.3.3",
    "vue-location-picker": "^1.0.1",
    "vue-range-component": "^1.0.3",
    "vue-select": "^3.10.3",
    "vue-trix": "^0.9.0",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.11"
